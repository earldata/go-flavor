<?xml version="1.0" encoding="UTF-8"?>
<flavor name="com.earldata.golangflavor" version="0.0.1" availability="public">
    <provider name="Tim Grant" url="http://github.com/earldata/go-flavor" email="tgrant@earldata.com" />
    <description>Import/include dependencies from Golang sources</description>

    <item-types>
        <module-types sep="/">
            <module-type name="package" icon="folder.gif" />
        </module-types>
        <nested-types sep="/">
            <nested-type name="type"/>
            <nested-type name="field"/>
            <nested-type name="function"/>
        </nested-types>
        <virtual-types>
            <virtual-type nature="root" icon="root.gif"/>
            <virtual-type nature="folder" name="package" icon="dir.gif"/>
            <virtual-type nature="module" icon="dir.gif"/>
            <virtual-type nature="file" icon="file.gif"/>
            <virtual-type nature="bundle" icon="bundle.gif"/>
        </virtual-types>
    </item-types>
    <dependency-types>
        <dependency-type name="imports"/>
    </dependency-types>
</flavor>